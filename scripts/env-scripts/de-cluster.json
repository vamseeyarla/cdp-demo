{
  "environmentName": "cdp-demo1",
  "tags": {
    "userDefined": {}
  },
  "authentication": {},
  "cluster": {
    "userName": "admin",
    "password": "admin123",
    "blueprintName": "CDP 1.0 - Data Engineering: Apache Spark, Apache Hive, Apache Oozie",
    "validateBlueprint": false,
    "cloudStorage": {
      "s3": {
        "instanceProfile": "arn:aws:iam::980678866538:instance-profile/Altus_Instance_Profile"
      },
      "locations": [
        {
          "propertyFile": "zeppelin-site",
          "propertyName": "zeppelin.notebook.s3.bucket",
          "value": "s3a://cloudbreak-group/user/rvenkatesh/dls/cdp-demo-dl-1-dev/rv-latest-de-1/zeppelin"
        }
      ]
    },
    "exposedServices": [
      "ALL"
    ]    
  },
  "parameters": {},
  "placement": {
    "region": "eu-west-1",
    "availabilityZone": "eu-west-1b"
  },
  "image": {
    "catalog": "cloudbreak-default",
    "os": "centos7"
  },
  "instanceGroups": [
    {
      "name": "master",
      "template": {
        "attachedVolumes": [
          {
            "count": 1,
            "size": 100,
            "type": "standard"
          }
        ],
