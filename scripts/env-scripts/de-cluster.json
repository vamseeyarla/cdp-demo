        "rootVolume": {
          "size": 50
        }
      },
      "nodeCount": 1,
      "type": "CORE",
      "recoveryMode": "MANUAL"
    },
    {
      "name": "compute",
      "template": {
        "attachedVolumes": [
          {
            "count": 1,
            "size": 100,
            "type": "standard"
          }
        ],
        "aws": {
          "encryption": {
            "type": "NONE"
          }
